{
  "name": "@sapn/elsa-util-deploy",
  "version": "0.35.0",
  "description": "elsa util for deploying (meta)data",
  "files": [
    "src/"
  ],
  "exports": {
    "./db/*": "./src/db/*.js",
    "./provisionBaselineTestTenant": "./src/provisionBaselineTestTenant.js",
    "./provisionTestTenant": "./src/provisionTestTenant.js",
    "./startDatabaseSetupJob": "./src/startDatabaseSetupJob.js",
    "./startDeployChangeHistoryMetadata": "./src/startDeployChangeHistoryMetadata.js",
    "./startProvisionClusterJob": "./src/startProvisionClusterJob.js",
    "./startProvisionTenantJob": "./src/startProvisionTenantJob.js",
    "./startProvisionTestContentJob": "./src/startProvisionTestContentJob.js"
  },
  "scripts": {
    "clean": "rimraf node_modules dist report",
    "coverage": "cross-env NODE_ENV=test c8 npm run test",
    "coverage:sonar": "cross-env NODE_ENV=test c8 --no-check-coverage npm run test",
    "lint": "elsa lint \"@(src|test)/**/*.{js,ts}\"",
    "lint:sonar": "elsa lint --scope sonar \"@(src|test)/**/*.{js,ts}\"",
    "lint:sonar:pr": "elsa lint --scope sonar:pr \"@(src|test)/**/*.{js,ts}\"",
    "test:mutation": "stryker run",
    "test": "mocha test",
    "publish:artifactory": "npm publish",
    "publish:staging": "cross-env-shell npm pack --pack-destination $npm_pack_destination"
  },
  "repository": {
    "type": "git",
    "url": "https://github.tools.sap/elsa/sap-n/tree/main/infrastructure/elsa/util/elsa-util-deploy"
  },
  "keywords": [
    "elsa"
  ],
  "author": {
    "name": "SAP SE"
  },
  "contributors": [],
  "elsa": {
    "owner": [
      "cam_c4c_elsa_core_member"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@sapn/elsa-util": "*",
    "@sapn/elsa-util-task": "*"
  },
  "devDependencies": {
    "@sapn/elsa-test": "*"
  },
  "nx": {
    "tags": [
      "scope:infrastructure"
    ]
  }
}
