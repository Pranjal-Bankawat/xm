{
	"lines": 100,
	"functions": 100,
	"branches": 100,
	"statements": 100,
	"reporter": [
		"lcov",
		"text",
		"cobertura"
	],
	"include": [
		"src/**/*.js"
	],
	"skip-empty": true,
	"skip-full": true,
	"sourceMap": false,
	"all": true,
	"check-coverage": true,
	"report-dir": "./report/coverage",
	"temp-directory": "./report/.c8_output"
}
